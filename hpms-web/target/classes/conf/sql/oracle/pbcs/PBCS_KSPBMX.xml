<?xml version="1.0" encoding="UTF-8"?>
<sqls xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../sql.xsd" namespace="PBCS_KSPBMX">
	<sql id="query">
		<dynamic>
			select
			LSH,YGBH,PBLSH,BBBM,BBJS,RQ,SJXQ,SJRQ,FJBB,FJJS,CJR,CJSJ,XGR,XGSJ,ZT
			from PBCS_KSPBMX where 1=1
